<!-- Table -->
<table id="myTable" class="table table-striped table-bordered" style="width:100%">
    <thead>
        <th>ID</th>
        <th>Product</th>
        <th>Qty</th>
        <th>Price</th>
    </thead>
    <tbody>
        <% products.forEach((product, index) => { %>
        
        <tr>
            <td><%= index+1 %></td>
            <td><%= product.name %></td>
            <td><%= product.qty %></td>
            <td><%= product.price %></td>
        </tr>
        
        <% }); %> 
    </tbody>
</table>