<!-- Table -->
<table id="myTable" class="table table-striped table-bordered" style="width:100%">
    <thead>
        <th>ID</th>
        <th>Product</th>
        <th>Qty</th>
        <th>Price</th>
    </thead>
    <tbody>
        <% items.forEach((item) => { %>
        
        <tr>
            <td><%= item._id %></td>
            <td><%= item.product %></td>
            <td><%= item.qty %></td>
            <td><%= item.price %></td>
        </tr>
        
        <% }); %> 
    </tbody>
</table>