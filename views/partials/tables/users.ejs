<!-- Table -->
<table id="myTable" class="table table-striped table-bordered" style="width:100%">
    <thead>
        <th>ID</th>
        <th>Username</th>
        <th>Role</th>
    </thead>
    <tbody>
        <% users.forEach((user, index) => { %>
        
        <tr>
            <td><%= user._id %></td>
            <td><%= user.username %></td>
            <td><%= user.role %></td>
        </tr>
        
        <% }); %> 
    </tbody>
</table>